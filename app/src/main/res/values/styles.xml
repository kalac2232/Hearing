<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="MainDragonballStyle" >
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:background">@drawable/shape_dragonball_bg</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="layout_constraintWidth_percent">0.12</item>
        <item name="layout_constraintDimensionRatio">1:1</item>
        <item name="layout_constraintLeft_toLeftOf">parent</item>
        <item name="layout_constraintRight_toRightOf">parent</item>
    </style>
    <style name="MainDragonbalTestlStyle" >
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#777</item>
    </style>
</resources>
